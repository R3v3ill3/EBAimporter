#############################################
# EA Importer Environment Configuration
# Copy to .env and fill in values
#############################################

# Database Configuration (Supabase Postgres recommended)
# Format:
# postgresql+psycopg2://USER:PASSWORD@HOST:PORT/DBNAME?sslmode=require
DB_URL=postgresql+psycopg2://postgres:YOUR_PASSWORD@db.PROJECT_REF.supabase.co:5432/postgres?sslmode=require
DB_ECHO=false
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=5

# Web Server
WEB_HOST=0.0.0.0
WEB_PORT=8080

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO
